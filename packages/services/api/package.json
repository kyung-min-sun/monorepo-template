{
  "name": "@project-template/api",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "oxlint src",
    "lint:fix": "oxlint --fix src",
    "typecheck": "tsc --noEmit",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src"
  },
  "devDependencies": {
    "@project-template/oxlint-config": "workspace:*",
    "@project-template/typescript-config": "workspace:*",
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@elysiajs/cors": "catalog:backend",
    "@elysiajs/eden": "catalog:backend",
    "@project-template/db": "workspace:*",
    "@project-template/server": "workspace:*",
    "@project-template/storage": "workspace:*",
    "@project-template/browser": "workspace:*",
    "zod": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./dist/src/client.d.ts",
      "default": "./dist/src/client.js"
    },
    "./server": {
      "types": "./dist/src/server.d.ts",
      "default": "./dist/src/server.js"
    }
  }
}
